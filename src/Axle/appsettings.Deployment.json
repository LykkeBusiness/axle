{
  "urls": "http://*:80;",
  "serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Async" ],
    "minimumLevel": {
      "default": "Information"
    },
    "writeTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "outputTemplate": "[{Timestamp:u}] [{Application}:{Version}:{Environment}] [{Level:u3}] [{RequestId}] [{CorrelationId}] [{ExceptionId}] {Message:lj} {NewLine}{Exception}"
              }
            },
            {
              "Name": "File",
              "Args": {
                "outputTemplate": "[{Timestamp:u}] [{Application}:{Version}:{Environment}] [{Level:u3}] [{RequestId}] [{CorrelationId}] [{ExceptionId}] {Message:lj} {NewLine}{Exception}",
                "rollingInterval": "Day",
                "path": "logs/Axle/Axle-deployment.log",
                "fileSizeLimitBytes": null
              }
            }
          ]
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "WithDemystifiedStackTraces" ],
    "Properties": {
      "Application": "Axle"
    },
    "CorsOrigins": [ "http://localhost:5013", "http://nova.lykkecloud.com" ],

    "SessionConfig": {
      "TimeoutInSec": 300
    }
  },
  "CorsOrigins": [ "http://localhost:5013", "http://nova.lykkecloud.com" ],
  "SessionConfig": {
    "TimeoutInSec": 300
  }
}